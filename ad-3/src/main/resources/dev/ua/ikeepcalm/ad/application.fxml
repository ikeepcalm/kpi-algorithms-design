                        <TableColumn fx:id="emailColumn" maxWidth="123" text="Email"/>
                        <TableColumn fx:id="usernameColumn" maxWidth="120" text="Username"/>
                        <TableColumn fx:id="passwordColumn" minWidth="100" text="Password"/>
